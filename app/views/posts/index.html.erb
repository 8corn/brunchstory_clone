<p id="notice"><%= notice %></p>

<h1>Posts</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
	  <h2>포스트 목록</h2>
		<% @category.posts.order(created_at: :desc).each do |post| %>
		  <div>
			<%= link_to post.title, category_post_path(@category, post) %>
		  </div>
		<% end %>
  </tbody>
</table>

<br>
